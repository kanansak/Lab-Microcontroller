void setup() {
  pinMode(13, OUTPUT);  //กำหนดภาวะ (Mode) ขา 13 เป็นขาส่งออก
}

void loop() {
 digitalWrite(13, LOW); //เขียน/ส่งออกค่าสัญญาณดิจิทัลทางขา 13 เป็น 0
  delay(250);    //หน่วง/ประวิงตามเวลาที่กำหนดไว้ก่อนทำงานบรรทัดถัดไป
  digitalWrite(13, HIGH); //เขียน/ส่งออกค่าสัญญาณดิจิทัลทางขา 13 เป็น 1
  delay(250);


}
